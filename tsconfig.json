{
    "compilerOptions": {
        "baseUrl": ".",
        "sourceRoot": "web_ui",
        "outDir": "dist",
        "module": "CommonJS",
        "target": "ESNext",
        "sourceMap": true,
        "useDefineForClassFields": true,
        "allowJs": false,
        "skipLibCheck": true,
        "declaration": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "isolatedModules": false,
        "suppressImplicitAnyIndexErrors": false,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true,
        "newLine": "LF",
        "noEmit": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "strict": true,
        "jsx": "react-jsx",
        "lib": ["ESNext", "DOM", "DOM.Iterable", "ESNext.AsyncIterable"],
        "typeRoots": ["./node_modules/@types"],
        "paths": {
            "@assets/*": ["web_ui/assets/*"],
            "@components": ["web_ui/components"],
            "@constants": ["web_ui/constants"],
            "@hooks": ["web_ui/hooks"],
            "@interfaces": ["web_ui/interfaces"],
            "@layouts": ["web_ui/layouts"],
            "@pages": ["web_ui/pages"],
            "@services": ["web_ui/services"],
            "@states": ["web_ui/states"],
            "@utils": ["web_ui/utils"],
            "#package.json": ["./package.json"]
        }
    },
    "include": ["web_ui", "**/*.js", "**/*.ts", "**/*.tsx", "*.cjs"],
    "exclude": ["node_modules", "dist", "coverage"],
    "linterOptions": {
        "exclude": []
    },
    "references": [{ "path": "./tsconfig.node.json" }]
}
