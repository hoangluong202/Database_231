{
  "name": "elearning-system",
  "version": "",
  "license": "ISC",
  "description": "",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig.compile.json && tsc-alias",
    "barrels": "barrelsby --config .barrelsby.json -q",
    "start:fe": "yarn barrels && cross-env NODE_ENV=development vite --host",
    "start:be": "yarn barrels && cross-env NODE_ENV=development tsnd --ignore-watch node_modules --respawn --transpile-only --max-old-space-size=2048 -r tsconfig-paths/register backend/index.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset --force",
    "db:deploy": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "lint": "eslint '**/*.{tsx,ts,js}'",
    "lint:fix": "eslint '**/*.{ts,js}' --fix",
    "format": "prettier '**/*.{tsx,ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install",
    "bootstrap": "yarn start:docker && yarn",
    "start:db": "yarn db:dropTable && yarn db:createTable && yarn db:createTrigger && yarn db:seedData",
    "start:docker": "docker compose -f docker-compose.dev.yml up -d",
    "clean:docker": "docker compose -f docker-compose.dev.yml down --volumes --remove-orphans",
    "clean:git": "git branch --merged >/tmp/merged-branches && nano /tmp/merged-branches && xargs git branch -D </tmp/merged-branches && git fetch --prune --all",
    "db:prisma:seed": "ts-node prisma/seed.ts",
    "db:crudReview": "ts-node postgres/procedure/crudReview.ts",
    "db:topCourse": "ts-node postgres/procedure/getTopCourse.ts",
    "db:filterAndSortCourse": "ts-node postgres/procedure/filterAndSortCourse.ts",
    "db:createTable": "ts-node postgres/1.1.1/createTable.ts",
    "db:dropTable": "ts-node postgres/1.1.1/dropTable.ts",
    "db:createTrigger": "ts-node postgres/1.2.2/createTrigger.ts",
    "db:seedData": "ts-node postgres/1.1.2/seedData.ts",
    "db:1.2.4.1": "ts-node postgres/1.2.4/highestRatingCourseFunc.ts",
    "db:1.2.4.2": "ts-node postgres/1.2.4/paidCourseRevenueFuc.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.4.2",
    "@fastify/helmet": "^11.1.1",
    "@fastify/sensible": "^5.5.0",
    "@fastify/swagger": "^8.12.1",
    "@fastify/swagger-ui": "^2.0.1",
    "@heroicons/react": "^2.0.18",
    "@material-tailwind/react": "^2.1.5",
    "@prisma/client": "^5.1.1",
    "@sinclair/typebox": "^0.31.28",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "fastify": "4.21.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.43",
    "pg": "^8.11.3",
    "prisma": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-toastify": "^9.1.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.4.8",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "barrelsby": "^2.8.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "pino-pretty": "^10.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.0.1",
    "tailwindcss": "^3.3.6",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.7",
    "vite-tsconfig-paths": "^4.2.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
